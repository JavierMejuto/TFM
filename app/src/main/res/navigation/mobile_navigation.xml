<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_fav">

    <fragment
        android:id="@+id/navigation_fav"
        android:name="com.example.trabajomaster.ui.favourite.FavouriteFragment"
        android:label="@string/title_fav"
        tools:layout="@layout/fragment_favourite" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.trabajomaster.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_leaderboard" />

    <fragment
        android:id="@+id/navigation_scenaries"
        android:name="com.example.trabajomaster.ui.scenaries.ScenariesFragment"
        android:label="@string/title_scenaries"
        tools:layout="@layout/fragment_scenaries" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.trabajomaster.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>